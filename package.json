{
  "name": "saucedemo-cypress",
  "version": "1.0.0",
  "description": "Cypress running against saucedemo application",
  "keywords": [
    "testing",
    "automation",
    "cypress",
    "saucedemo"
  ],
  "scripts": {
    "test": "cypress run --browser chrome",
    "test:stage": "cypress run --config baseUrl=http://stage.saucedemo.com/ --browser chrome",
    "test:report": "cypress run --reporter mochawesome"
  },
  "author": "Tulio Bandeira Luz",
  "devDependencies": {
    "cypress": "^12.12.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}